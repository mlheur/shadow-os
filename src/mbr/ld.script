ENTRY(_start)
SECTIONS {
    .text : {
        _start = .;
        mbr.o(.text)
        . = 0x1BE;
        mbr_partition_table = .;
        partition_table.o(.text)
        . = 0x1FE;
        mbr_signature = .;
        mbr_signature.o(.text)
    }
    /DISCARD/ : { *(.data) *(.bss) }
}
OUTPUT_ARCH(i386)
OUTPUT_FORMAT(binary)